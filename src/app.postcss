/* Write your global styles here, in PostCSS syntax */
@tailwind base;
@tailwind components;
@tailwind utilities;

@layer base {
	@font-face {
		font-family: 'Lato';
		src: url('/fonts/Lato-Regular.ttf');
		font-weight: normal;
		font-display: swap;
	}
	@font-face {
		font-family: 'Lato';
		src: url('/fonts/Lato-Bold.ttf');
		font-weight: bold;
		font-display: swap;
	}
}
